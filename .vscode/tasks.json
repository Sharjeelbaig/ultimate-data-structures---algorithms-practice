			"options": {
				"cwd": "D:/Installed Softwares/mingw32/bin"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build",
			"detail": "compiler: \"D:/Installed Softwares/mingw32/bin/g++.exe\""
		}
	]
